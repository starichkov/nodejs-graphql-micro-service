{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["src", "README.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "destination": "./out-docs",
        "tutorials": "./docs"
    }
}
